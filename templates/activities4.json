{
  "apiEndpoint": "activities",
  "apiPaging": "random",
  "apiVersion": 3,
  "fileName": "dm-activities-faild-job.csv",
  "sortField": "createdAt",
  "sortOrder": "DESC",
  "lookBack": 1,
  "lookBackUnit": "days",
  "lookBackFormat": "yyyy-MM-ddTHH:mm:ss.fffZ",
  "filters": [
    "(result.status in (\"FAILED\") ",
    "or state in (\"FAILED\"))",
    "and category eq \"BACKUP\"",
    "and parentId eq null",
    "and classType in (\"JOB\", \"JOB_GROUP\")",
    "and startedAt gt \"2025-10-28T18:43:30Z\""
  ],
  "fields": [
    {
      "label": "displayId",
      "value": "displayId"
    },
    {
      "label": "description",
      "value": "description"
    },
    {
      "label": "classType",
      "value": "classType"
    },
    {
      "label": "category",
      "value": "category"
    },
    {
      "label": "createdAt",
      "value": "createdAt",
      "format": "local"
    },
    {
      "label": "startedAt",
      "value": "startedAt",
      "format": "local"
    },
    {
      "label": "completedAt",
      "value": "completedAt",
      "format": "local"
    },
    {
      "label": "policyName",
      "value": "references|?usage eq PROTECTION_POLICY|name"
    },
    {
      "label": "storageTarget",
      "value": "references|?usage eq DATA_TARGET|name"
    },
    {
      "label": "state",
      "value": "state"
    },
    {
      "label": "status",
      "value": "result.status"
    }
  ]
}
