{
  "apiEndpoint": "assets",
  "apiPaging": "random",
  "apiVersion": 2,
  "fileName": "dm-assets-filesystem.csv",
  "sortField": "name",
  "sortOrder": "ASC",
  "lookBack": 1,
  "lookBackUnit": "days",
  "lookBackFormat": "yyyy-MM-ddTHH:mm:ss.fffZ",
  "filters": [
    "type eq \"FILE_SYSTEM\"",
    "and lastDiscoveryStatus in (\"NEW\",\"DETECTED\",\"NOT_DETECTED\")"
  ],
  "fields": [
    {
      "label": "name",
      "value": "name"
    },
    {
      "label": "status",
      "value": "status"
    },
    {
      "label": "type",
      "value": "type"
    },
    {
      "label": "clusterName",
      "value": "details.fileSystem.clusterName"
    },
    {
      "label": "clusterType",
      "value": "details.fileSystem.clusterType"
    },
    {
      "label": "instance",
      "value": "details.fileSystem.appServerName"
    },
    {
      "label": "policyName",
      "value": "protectionPolicy.name"
    },
    {
      "label": "policyType",
      "value": "protectionPolicy.type"
    },
    {
      "label": "protectionStatus",
      "value": "protectionStatus"
    },
    {
      "label": "discoveredSize",
      "value": "size",
      "format": "base10"
    },
    {
      "label": "licensedSize",
      "value": "protectionCapacity.size",
      "format":"base10"
    },
    {
      "label": "lastAvailableCopyTime",
      "value": "lastAvailableCopyTime"
    }
  ]
}
